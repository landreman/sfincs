#!/bin/bash -l

# Standard output and error:
#SBATCH -o ./sfincsJob.out.%j
#SBATCH -e ./sfincsJob.err.%j
# Initial working directory:
#SBATCH -D ./
# Job Name:
#SBATCH -J sfincs

#SBATCH --nodes=1
#SBATCH --ntasks-per-node=48

# Wall clock limit:
#SBATCH --time=00:10:00

#SBATCH --mem 100000

srun ../../sfincs -ksp_view
